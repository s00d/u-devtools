{
  "name": "@u-devtools/plugin-storage",
  "version": "0.1.0",
  "description": "Storage Manager plugin for viewing and editing LocalStorage, SessionStorage, and Cookies",
  "type": "module",
  "main": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js"
    },
    "./app": {
      "types": "./dist/app.d.ts",
      "import": "./dist/app.js"
    }
  },
  "files": [
    "dist"
  ],
  "publishConfig": {
    "main": "./dist/index.js",
    "types": "./dist/index.d.ts"
  },
  "scripts": {
    "build": "vite build",
    "typecheck": "tsc --noEmit",
    "prepublishOnly": "pnpm build"
  },
  "dependencies": {
    "@u-devtools/core": "workspace:*",
    "@u-devtools/kit": "workspace:*",
    "@u-devtools/ui": "workspace:*",
    "@vueuse/core": "^14.1.0",
    "idb": "^8.0.3",
    "universal-cookie": "^7",
    "vue": "^3.5.26"
  },
  "devDependencies": {
    "@types/node": "^20.19.27",
    "typescript": "^5.9.3"
  }
}
