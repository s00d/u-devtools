    "@u-devtools/utils": "workspace:*",
    "@u-devtools/kit": "workspace:*",
    "@vue/devtools-core": "^8.0.5",
    "@vue/devtools-kit": "^8.0.5",
    "vite-plugin-vue-inspector": "^5.3.2",
    "vue": "^3.5.26"
