{
  "name": "@u-devtools/plugin-vue-inspector",
  "version": "0.1.3",
  "description": "Vue Inspector plugin for Universal DevTools",
  "type": "module",
  "main": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js"
    },
    "./app": {
      "types": "./dist/app.d.ts",
      "import": "./dist/app.js"
    },
    "./package.json": "./package.json"
  },
  "files": [
    "dist",
    "src",
    "README.md"
  ],
  "keywords": [
    "u-devtools-plugin",
    "devtools",
    "vue",
    "inspector",
    "components",
    "vue3"
  ],
  "author": "",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/s00d/u-devtools.git",
    "directory": "plugins/vue-inspector"
  },
  "publishConfig": {
    "access": "public",
    "main": "./dist/index.js",
    "module": "./dist/index.js",
    "types": "./dist/index.d.ts",
    "exports": {
      ".": {
        "types": "./dist/index.d.ts",
        "import": "./dist/index.js"
      },
      "./app": {
        "types": "./dist/app.d.ts",
        "import": "./dist/app.js"
      }
    }
  },
  "scripts": {
    "build": "vite build",
