{
  "name": "@u-devtools/plugin-vue-inspector",
  "version": "0.1.0",
  "description": "Vue Inspector plugin for Universal DevTools",
  "type": "module",
  "main": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js"
    },
    "./app": {
      "types": "./dist/app.d.ts",
      "import": "./dist/app.js"
    },
    "./plugin-vue-inspector.css": "./dist/plugin-vue-inspector.css",
    "./style.css": "./dist/plugin-vue-inspector.css",
    "./main.css": "./dist/plugin-vue-inspector.css",
    "./package.json": "./package.json"
  },
  "files": [
    "dist"
  ],
  "publishConfig": {
    "access": "public",
    "main": "./dist/index.js",
    "module": "./dist/index.js",
    "types": "./dist/index.d.ts",
    "exports": {
      ".": {
        "types": "./dist/index.d.ts",
        "import": "./dist/index.js"
      },
      "./app": {
        "types": "./dist/app.d.ts",
        "import": "./dist/app.js"
      }
    }
  },
  "scripts": {
    "build": "vite build",
    "typecheck": "tsc --noEmit",
    "prepublishOnly": "pnpm build"
  },
  "dependencies": {
    "@u-devtools/core": "workspace:*",
    "@u-devtools/ui": "workspace:*",
    "@u-devtools/utils": "workspace:*",
    "@u-devtools/kit": "workspace:*",
    "@vue/devtools-core": "^8.0.5",
    "@vue/devtools-kit": "^8.0.5",
    "vite-plugin-vue-inspector": "^5.3.2",
    "vue": "^3.5.26"
  },
  "devDependencies": {
    "@types/node": "^20.19.27",
    "typescript": "^5.9.3"
  }
}
