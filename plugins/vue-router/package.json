{
  "name": "@u-devtools/plugin-vue-router",
  "version": "0.1.0",
  "description": "Vue Router inspector plugin for Universal DevTools",
  "type": "module",
  "main": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js"
    },
    "./app": {
      "types": "./dist/app.d.ts",
      "import": "./dist/app.js"
    }
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "vite build",
    "dev": "vite build --watch",
    "typecheck": "tsc --noEmit"
  },
  "dependencies": {
    "@u-devtools/core": "workspace:*",
    "@u-devtools/ui": "workspace:*",
    "vue": "^3.5.26"
  },
  "devDependencies": {
    "@types/node": "^20.19.27",
    "@vitejs/plugin-vue": "^6.0.3",
    "typescript": "^5.9.3",
    "vite": "^7.3.0",
    "vite-plugin-dts": "^4.5.4"
  }
}

